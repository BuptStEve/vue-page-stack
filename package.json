{
  "name": "vue-page-stack",
  "version": "3.0.0",
  "description": "Routing and navigation for your Vue SPA. Vue 单页应用导航管理器",
  "author": "hezf",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "cz": "git-cz"
  },
  "dependencies": {},
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.0.4",
    "@vue/compiler-sfc": "^3.0.5",
    "@vuedx/typecheck": "^0.4.1",
    "@vuedx/typescript-plugin-vue": "^0.4.1",
    "vue": "^3.0.5",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.35",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-angular": "^11.0.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^4.3.8"
  },
  "keywords": [
    "vue",
    "vue 3.0",
    "vue-router",
    "stack",
    "navigation",
    "manager",
    "page"
  ],
  "files": [
    "dist"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "types": {
        "feat": {
          "description": "A new feature",
          "title": "Features"
        },
        "fix": {
          "description": "A bug fix",
          "title": "Bug Fixes"
        },
        "docs": {
          "description": "Documentation only changes",
          "title": "Documentation"
        },
        "style": {
          "description": "Changes that do not affect the meaning of the code (white-space, formatting, missing semi-colons, etc)",
          "title": "Styles"
        },
        "refactor": {
          "description": "A code change that neither fixes a bug nor adds a feature",
          "title": "Code Refactoring"
        },
        "perf": {
          "description": "A code change that improves performance",
          "title": "Performance Improvements"
        },
        "test": {
          "description": "Adding missing tests or correcting existing tests",
          "title": "Tests"
        },
        "chore": {
          "description": "Other changes that don't modify src or test files",
          "title": "Chores"
        }
      }
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hezhongfeng/vue-page-stack.git"
  },
  "bugs": {
    "url": "https://github.com/hezhongfeng/vue-page-stack/issues"
  },
  "homepage": "https://github.com/hezhongfeng/vue-page-stack#readme",
  "main": "./dist/my-lib.umd.js",
  "module": "./dist/my-lib.es.js",
  "exports": {
    ".": {
      "import": "./dist/my-lib.es.js",
      "require": "./dist/my-lib.umd.js"
    }
  }
}
